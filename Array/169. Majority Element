class Solution {
    public int majorityElement(int[] nums) {
        
        int element = nums[0];
        int count = 0;
        
        for(int i=0;i<nums.length;i++){
            if(nums[i]==element){
                count++;
            }else{
                if(count>0){
                    count--;
                }else{
                    element = nums[i];
                }
            }
            
        }
        return element;
    }
}
